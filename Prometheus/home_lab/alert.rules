groups:
- name: test
  rules:
  - alert: Host is dowm
    expr: up == 0
    for: 0m
    labels:
      severity: critical
    annotations:
      summary: "Host (instance {{ $labels.instance }}) is not available"
      description: "A Prometheus target has disappeared. An exporter might be crashed. VALUE = {{ $value }}  LABELS: {{ $labels }}"
